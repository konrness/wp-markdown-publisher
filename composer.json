    "require": {
        "php": ">=5.3",
        "nerdery/wp-plugin-framework": "dev-master",
        "fabricius/fabricius": "~1.0",
        "michelf/php-markdown": "~1.4"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "MarkdownPublisher": "src/"
